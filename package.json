{
  "name": "std-node",
  "version": "1.0.0",
  "description": "node project start",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node ./lib/index.js",
    "start-dev": "NODE_ENV=dev nodemon --exec babel-node ./src/index.js",
    "start-stage": "NODE_ENV=stage nodemon --exec babel-node ./src/index.js",
    "start1": "nodemon --exec babel-node ./src/examples/class.js",
    "build": "node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "hint": "eslint src",
    "push": "git add . && git commit -m'mod' &&  git push origin master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weizhou2000/std-node.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/weizhou2000/std-node/issues"
  },
  "homepage": "https://github.com/weizhou2000/std-node#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1"
  }
}
